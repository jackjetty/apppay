<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout android:layout_width="fill_parent" android:layout_height="fill_parent"
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:background="@color/main_bg_color" >
   <include 
        android:id="@+id/topTitle" 
        layout="@layout/title_bar"  
     /> 
      
              
     <ScrollView
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:fadingEdge="none"
        android:scrollbars="vertical" 
        android:layout_below="@id/topTitle"
         >
          
        
         <LinearLayout 
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:focusable="true"
            android:focusableInTouchMode="true"
            android:orientation="vertical" >
            
		   
		  
		  
		  
           
          <LinearLayout 
               android:gravity="center_vertical"  
                android:background="@drawable/settings_mid" 
                
                android:padding="10.0dip" 
                android:layout_marginTop="@dimen/row_margin"  
                android:layout_width="fill_parent" 
                android:layout_height="@dimen/row_height" 
                >
		        <TextView 
		             
		            android:textSize="@dimen/textSize_normal" 
		            android:textColor="@color/key_gray"
		            android:paddingLeft="5.0dip" 
		            android:layout_width="90dip"
		            android:layout_height="wrap_content" 
		            android:text="缴费编号" />
		        <TextView 
		            android:id="@+id/tv_DealNumber"
		              android:textSize="@dimen/textSize_normal" 
		             android:textColor="@color/value_gray"  
		             android:background="@null" 
		             android:layout_width="fill_parent"
		             android:layout_height="wrap_content"  
		             android:gravity="right"
		             android:text="21642164121651552" 
		             
                  />
		  </LinearLayout>
		  
          <LinearLayout 
               android:gravity="center_vertical"  
                android:background="@drawable/settings_mid" 
                android:padding="10.0dip"  
                android:layout_width="fill_parent" 
                android:layout_height="@dimen/row_height" 
                >
		        <TextView 
		            android:textSize="@dimen/textSize_normal" 
		            android:textColor="@color/key_gray"
		            android:paddingLeft="5.0dip" 
		            android:layout_width="90dip"
		            android:layout_height="wrap_content" 
		            android:text="缴费项目" />
		        <TextView 
		            android:id="@+id/tv_ApplyTitle"
		            android:textSize="@dimen/textSize_normal" 
		            android:textColor="@color/value_gray"  
		            android:background="@null" 
		            android:layout_width="fill_parent"
		             android:layout_height="wrap_content" 
		             
		             android:gravity="right"
		             android:text="大学英语书记" 
		             
                  />
		  </LinearLayout>
		  <LinearLayout 
               android:gravity="center_vertical"  
                android:background="@drawable/settings_mid" 
                android:padding="10.0dip"  
                android:layout_width="fill_parent" 
                android:layout_height="@dimen/row_height" 
                >
		        <TextView 
		            android:textSize="@dimen/textSize_normal" 
		            android:textColor="@color/key_gray"
		            android:paddingLeft="5.0dip" 
		            android:layout_width="90dip"
		            android:layout_height="wrap_content" 
		            android:text="缴费金额" />
		        <TextView 
		            android:id="@+id/tv_ApplyAmountName"
		            android:textSize="@dimen/textSize_normal" 
		            android:textColor="@color/value_gray"  
		            android:background="@null" 
		            android:layout_width="fill_parent"
		             android:layout_height="wrap_content"  
		             android:gravity="right"
		             android:text="120.00元" 
		             
                  />
		  </LinearLayout>
		  
		  <LinearLayout 
               android:gravity="center_vertical"  
                android:background="@drawable/settings_mid" 
                android:padding="10.0dip"  
                android:layout_width="fill_parent" 
                android:layout_height="@dimen/row_height" 
                >
		        <TextView 
		            android:textSize="@dimen/textSize_normal" 
		            android:textColor="@color/key_gray"
		            android:paddingLeft="5.0dip" 
		            android:layout_width="90dip"
		            android:layout_height="wrap_content" 
		            android:text="缴费单编号" />
		        <TextView 
		            android:id="@+id/tv_PayNo"
		            android:textSize="@dimen/textSize_normal" 
		            android:textColor="@color/value_gray"  
		            android:background="@null" 
		            android:layout_width="fill_parent"
		             android:layout_height="wrap_content"  
		             android:gravity="right"
		             android:text="12312321" 
		             
                  />
		  </LinearLayout>
           <LinearLayout
		            android:layout_width="fill_parent"
		            android:layout_height="@dimen/row_height"
		            android:background="@drawable/settings_mid"
		            android:gravity="center_vertical"
		            android:padding="10.0dip" >

		            <TextView
		                android:layout_width="90dip"
		                android:layout_height="wrap_content"
		                android:paddingLeft="5.0dip"
		                android:text="缴费结果"
		                android:textColor="@color/key_gray"
		                android:textSize="@dimen/textSize_normal" />

		            <TextView
		                android:id="@+id/tv_Status"
		                android:layout_width="fill_parent"
		                android:layout_height="wrap_content"
		                android:background="@null"
		                android:gravity="right"
		                android:text="缴费成功"
		                android:textColor="@color/value_gray"
		                android:textSize="@dimen/textSize_normal" />
		        </LinearLayout>
		         <LinearLayout 
                android:gravity="center_vertical"  
                android:background="@drawable/settings_mid" 
                android:padding="10.0dip"  
                android:layout_width="fill_parent" 
                android:layout_height="@dimen/row_height" 
                >
		        <TextView 
		            android:textSize="@dimen/textSize_normal" 
		            android:textColor="@color/key_gray"
		            android:paddingLeft="5.0dip" 
		            android:layout_width="90dip"
		            android:layout_height="wrap_content" 
		            android:text="缴费时间" />
		        <TextView 
		            android:id="@+id/tv_EnterTime"
		            android:textSize="@dimen/textSize_normal" 
		            android:textColor="@color/value_gray"  
		            android:background="@null" 
		            android:layout_width="fill_parent"
		             android:layout_height="wrap_content"  
		             android:gravity="right"
		             android:text="12312321" 
		             
                  />
		  </LinearLayout>
          <LinearLayout 
               android:gravity="center_vertical"  
                android:background="@drawable/settings_mid" 
                android:padding="10.0dip"  
                android:layout_width="fill_parent" 
                android:layout_height="@dimen/row_height" 
                >
		        <TextView 
		            android:textSize="@dimen/textSize_normal" 
		            android:textColor="@color/key_gray"
		            android:paddingLeft="5.0dip" 
		            android:layout_width="90dip"
		            android:layout_height="wrap_content" 
		            android:text="姓名" />
		        <TextView 
		            android:id="@+id/tv_UserName"
		            android:textSize="@dimen/textSize_normal" 
		            android:textColor="@color/value_gray"  
		            android:background="@null" 
		            android:layout_width="fill_parent"
		             android:layout_height="wrap_content"  
		             android:gravity="right"
		             android:text="张三" 
		             
                  />
               </LinearLayout>
		        
 
		  
          
          <LinearLayout 
                android:id="@+id/ll_Other1" 
                android:gravity="center_vertical"  
                android:background="@drawable/settings_mid" 
                android:padding="10.0dip" 
                android:layout_width="fill_parent" 
                android:layout_height="wrap_content" 
                android:minHeight="@dimen/row_height" 
                >
		        <TextView 
		            android:id="@+id/tv_LabelOther1" 
		            android:textSize="@dimen/textSize_normal" 
		            android:textColor="@color/key_gray"
		            android:paddingLeft="5.0dip" 
		            android:layout_width="90dip"
		            android:layout_height="wrap_content" 
		            android:text="其他一" />
		        <TextView 
		            android:id="@+id/tv_ValueOther1"
		            android:textSize="@dimen/textSize_normal" 
		            android:maxLines="2" 
		            android:background="@null" 
		            android:layout_width="fill_parent"
		             android:layout_height="wrap_content" 
		             android:layout_marginLeft="10.0dip" 
		             android:textColor="@color/value_gray"
		             android:gravity="right" 
		             android:text="请输入 " 
		             
                  />
		  </LinearLayout>
           <LinearLayout 
                android:id="@+id/ll_Other2" 
                android:gravity="center_vertical"  
                android:background="@drawable/settings_mid" 
                android:padding="10.0dip" 
                android:layout_width="fill_parent" 
                 android:layout_height="wrap_content" 
                android:minHeight="@dimen/row_height" 
                >
		        <TextView  
		            android:id="@+id/tv_LabelOther2" 
		            android:textSize="@dimen/textSize_normal" 
		            android:textColor="@color/key_gray"
		            android:paddingLeft="5.0dip" 
		            android:layout_width="90dip"
		            android:layout_height="wrap_content" 
		            android:text="其他二" />
		        <TextView  
		            android:id="@+id/tv_ValueOther2"
		            android:textSize="@dimen/textSize_normal" 
		             android:maxLines="2"  
		            android:background="@null" 
		            android:layout_width="fill_parent"
		            android:layout_height="wrap_content" 
		            android:layout_marginLeft="10.0dip" 
		            android:textColor="@color/value_gray"
		            android:gravity="right" 
		            android:text="请输入" 
		             
                  />
		  </LinearLayout>
          <LinearLayout 
               android:gravity="center_vertical"  
                android:background="@drawable/settings_mid" 
                android:padding="10.0dip"
                android:layout_width="fill_parent" 
                 android:layout_height="wrap_content" 
                android:minHeight="@dimen/row_height" 
                >
		        <TextView 
		            android:textSize="@dimen/textSize_normal" 
		            android:textColor="@color/key_gray"
		            android:paddingLeft="5.0dip" 
		            android:layout_width="90dip"
		            android:layout_height="wrap_content" 
		            android:text="备注说明" />
		        <TextView 
		            android:id="@+id/tv_Remark"
		            android:textSize="@dimen/textSize_normal"  
		            android:background="@null" 
		            android:layout_width="fill_parent"
		            android:layout_height="wrap_content" 
		            android:layout_marginLeft="10.0dip"  
		            android:textColor="@color/value_gray"
		            android:gravity="right" 
		            android:maxLines="3" 
		            android:text="请输入备注信息"
		             
                  />
		  </LinearLayout>
		  
           <ImageView
                android:layout_width="match_parent"
                android:layout_height="1px"
                android:background="@drawable/divider" />
            
		  
            
           
           
           <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="30dip"
                android:layout_margin="20dip"
                android:gravity="center_vertical"
                android:orientation="horizontal" >  
                <TextView
                    android:id="@+id/tv_ResInstruction"
                    android:paddingLeft="3dip"
                    android:textSize="14dip" 
                    android:textColor="@color/value_gray"
                    android:text="说明：\n         新充值的校园一卡通需要到校园充值处刷卡"
                    android:gravity="center_vertical"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1" /> 
            </LinearLayout>
		   
         </LinearLayout>
    </ScrollView> 
</RelativeLayout>