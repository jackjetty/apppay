<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="90.0dip" />
            <gradient android:startColor="#CFCFCF" android:endColor="#CFCFCF" android:angle="270.0" android:centerY="0.75" android:centerColor="#CFCFCF" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="90.0dip" />
                <gradient android:startColor="#ff1b9cff" android:endColor="#ff1b9cff" android:angle="270.0" android:centerY="0.75" android:centerColor="#ff1b9cff" />
            </shape>
        </clip>
    </item>
</layer-list>