<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/dialog"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginLeft="15dp"
    android:layout_marginRight="15dp"
   
    android:background="@drawable/fynn_prompt_dialog_bg_normal"
    android:orientation="vertical" >

    <LinearLayout
        android:id="@+id/titlebar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/fynn_prompt_dialog_title_bar_bg_skyblue"
        android:gravity="left|center_vertical"
        android:paddingBottom="10dp"
        android:paddingLeft="15dp"
        android:paddingRight="15dp"
        android:paddingTop="10dp" >

        <TextView
            android:id="@+id/title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:gravity="center"
            android:textColor="#FFFFFF"
            android:textSize="20sp" 
            android:text="缴费支付确认"/>
    </LinearLayout>

    <View
        android:id="@+id/title_msg_divider"
        android:layout_width="match_parent"
        android:layout_height="1.0px"
        android:layout_marginBottom="10dp"
        android:background="#DCDCDC"
        android:visibility="visible" />
 <ScrollView
        android:id="@+id/sv_Dialog"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:fadingEdge="none"
        android:scrollbars="vertical"  >
    <LinearLayout
        android:id="@+id/layout_addview"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="10dip"
        android:layout_marginLeft="5.0dp"
        android:layout_marginRight="5.0dp"
        android:gravity="center"
        android:orientation="vertical" >
        
        <LinearLayout 
                android:gravity="center"   
                android:padding="8.0dip"   
                android:layout_width="fill_parent" 
                android:layout_height="wrap_content" 
                >
		        <TextView  
		            android:textSize="@dimen/textSize_large" 
		            android:textColor="@color/key_gray"
		            android:paddingLeft="5.0dip" 
		            android:layout_width="80dip"
		            android:layout_height="wrap_content" 
		            android:text="缴费项目" />
		        <TextView 
		            android:id="@+id/tv_ApplyTitle"
		            android:textSize="@dimen/textSize_normal" 
		            android:textColor="#696969"
		            android:background="@null" 
		            android:layout_width="fill_parent"
	                android:layout_height="wrap_content" 
	                android:layout_marginLeft="8.0dip" 
	                android:gravity="center"
	                android:text="全国英语司机四大罚款的看法考试" 
		            
                  />
		  </LinearLayout>
		  <LinearLayout  
                android:gravity="center"   
                android:padding="8.0dip"   
                android:layout_width="fill_parent" 
                android:layout_height="wrap_content" 
                >
		        <TextView  
		            android:id="@+id/tv_SkuLabel"
		            android:textSize="@dimen/textSize_large" 
		            android:textColor="@color/key_gray"
		            android:paddingLeft="5.0dip" 
		            android:layout_width="80dip"
		            android:layout_height="wrap_content" 
		            android:text="报名等级" />
		        <TextView 
		            android:id="@+id/tv_SkuName"
		            android:textSize="@dimen/textSize_normal" 
		            android:textColor="#696969"
		            android:background="@null" 
		            android:layout_width="fill_parent"
	                android:layout_height="wrap_content" 
	                android:layout_marginLeft="8.0dip" 
	                android:gravity="center"
	                android:text="一级" 
		               
                  />
		  </LinearLayout>
          <LinearLayout 
                android:gravity="center"   
                android:padding="8.0dip"   
                android:layout_width="fill_parent" 
                android:layout_height="wrap_content" 
                >
		        <TextView  
		            android:textSize="@dimen/textSize_large" 
		            android:textColor="@color/key_gray"
		            android:paddingLeft="5.0dip" 
		            android:layout_width="80dip"
		            android:layout_height="wrap_content" 
		            android:text="缴费金额" />
		        <TextView 
		            android:id="@+id/tv_ApplyAmount"
		            android:textSize="@dimen/textSize_normal" 
		            android:textColor="#696969"
		            android:background="@null" 
		            android:layout_width="fill_parent"
	                android:layout_height="wrap_content" 
	                android:layout_marginLeft="8.0dip" 
	                android:gravity="center"
	                android:text="50元" 
		              
                  />
		  </LinearLayout>
		  <LinearLayout 
                android:gravity="center"   
                android:padding="8.0dip"   
                android:layout_width="fill_parent" 
                android:layout_height="wrap_content" 
                >
		        <TextView  
		            android:textSize="@dimen/textSize_large" 
		            android:textColor="@color/key_gray"
		            android:paddingLeft="5.0dip" 
		            android:layout_width="80dip"
		            android:layout_height="wrap_content" 
		            android:text="缴费号" />
		        <TextView 
		            android:id="@+id/tv_PayNum"
		            android:textSize="@dimen/textSize_normal" 
		            android:textColor="#696969"
		            android:background="@null" 
		            android:layout_width="fill_parent"
	                android:layout_height="wrap_content" 
	                android:layout_marginLeft="8.0dip" 
	                android:gravity="center"
	                android:text="12312312" 
		                
                  />
		  </LinearLayout>
         <LinearLayout 
                android:gravity="center"   
                android:padding="8.0dip"   
                android:layout_width="fill_parent" 
                android:layout_height="wrap_content" 
                >
		        <TextView  
		            android:textSize="@dimen/textSize_large" 
		            android:textColor="@color/key_gray"
		            android:paddingLeft="5.0dip" 
		            android:layout_width="80dip"
		            android:layout_height="wrap_content" 
		            android:text="姓名" />
		        <TextView 
		            android:id="@+id/tv_UserName"
		            android:textSize="@dimen/textSize_normal" 
		            android:textColor="#696969"
		            android:background="@null" 
		            android:layout_width="fill_parent"
	                android:layout_height="wrap_content" 
	                android:layout_marginLeft="8.0dip" 
	                android:gravity="center"
	                android:text="张三" 
		               
                  />
		  </LinearLayout>
		   <LinearLayout
		        android:id="@+id/ll_Other1"  
                android:gravity="center"   
                android:padding="8.0dip"   
                android:layout_width="fill_parent" 
                android:layout_height="wrap_content"  
                >
		        <TextView  
		            android:id="@+id/tv_LabelOther1" 
		            android:textSize="@dimen/textSize_large" 
		            android:textColor="@color/key_gray"
		            android:paddingLeft="5.0dip" 
		            android:layout_width="80dip"
		            android:layout_height="wrap_content" 
		            android:text="其他一" />
		        <TextView 
		            android:id="@+id/tv_ValueOther1"
		            android:textSize="@dimen/textSize_normal" 
		            android:textColor="#696969"
		            android:background="@null" 
		            android:layout_width="fill_parent"
	                android:layout_height="wrap_content" 
	                android:layout_marginLeft="8.0dip" 
	                android:gravity="center"
	                android:text="士大夫撒旦" 
		               
                  />
		  </LinearLayout>
		  <LinearLayout
		        android:id="@+id/ll_Other2"  
                android:gravity="center"   
                android:padding="8.0dip"   
                android:layout_width="fill_parent" 
                android:layout_height="wrap_content"  
                >
		        <TextView  
		            android:id="@+id/tv_LabelOther2" 
		            android:textSize="@dimen/textSize_large" 
		            android:textColor="@color/key_gray"
		            android:paddingLeft="5.0dip" 
		            android:layout_width="80dip"
		            android:layout_height="wrap_content" 
		            android:text="其他二" />
		        <TextView 
		            android:id="@+id/tv_ValueOther2"
		            android:textSize="@dimen/textSize_normal" 
		            android:textColor="#696969"
		            android:background="@null" 
		            android:layout_width="fill_parent"
	                android:layout_height="wrap_content" 
	                android:layout_marginLeft="8.0dip" 
	                android:gravity="center"
	                android:text="士大夫撒旦" 
		              
                  />
		  </LinearLayout>
		  
		  
		  <LinearLayout  
                android:gravity="center"   
                android:padding="8.0dip"   
                android:layout_width="fill_parent" 
                android:layout_height="wrap_content"  
                >
		        <TextView   
		            android:textSize="@dimen/textSize_large" 
		            android:textColor="@color/key_gray"
		            android:paddingLeft="5.0dip" 
		            android:layout_width="80dip"
		            android:layout_height="wrap_content" 
		            android:text="备注说明" />
		        <TextView 
		            android:id="@+id/tv_Remark"
		            android:textSize="@dimen/textSize_normal" 
		            android:textColor="#696969"
		            android:background="@null" 
		            android:layout_width="fill_parent"
	                android:layout_height="wrap_content" 
	                android:layout_marginLeft="8.0dip" 
	                android:gravity="center"
	                android:text="士大夫撒旦" 
		              
                  />
		  </LinearLayout>
    </LinearLayout>

    </ScrollView>

    

    <View
        android:id="@+id/msg_btn_divider"
        android:layout_width="match_parent"
        android:layout_height="1.0px"
        android:background="#DCDCDC"
          />

    <LinearLayout
        android:id="@+id/btn_view"
        android:layout_width="match_parent"
        android:layout_height="50dp" >

        <TextView
            android:id="@+id/button_left"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@drawable/fynn_prompt_dialog_btn_left_selector"
            android:clickable="true"
            android:gravity="center"
            android:textColor="#808080"
            android:textSize="18sp"
            android:text="取消"
            android:singleLine="true" 
		            android:ellipsize="end" 
             />

        <View
            android:id="@+id/btn_divider"
            android:layout_width="1.0px"
            android:layout_height="wrap_content"
            android:background="#DCDCDC"
             /> 
        <TextView
            android:id="@+id/button_right"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@drawable/fynn_prompt_dialog_btn_right_selector"
            android:clickable="true"
            android:gravity="center"
            android:textColor="#808080"
            android:textSize="18sp"
            android:text="确认"
             />
    </LinearLayout>

</LinearLayout>